<launch>
	 <arg name="fcu_url" default="udp://127.0.0.1:14551@14555" />
	 <param name="name" value="$(arg fcu_url)"/>
     <arg name="server_ip" default="54.161.15.175"/>
     <param name="server_ip" value="$(arg server_ip)"/>
     <arg name="name" default="hexacopter"/>
     <param name="name" value="$(arg name)"/>
     <group ns="$(arg name)">
        <node pkg="server_connector" name="connect_to_server" type="connect_to_server.py" output="screen"/>
     </group>
</launch>
